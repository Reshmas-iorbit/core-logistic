{
  "division": {
    "name": "PO Header",
    "formelements": [
      {
        "id": "POID",
        "label": "PO ID",
        "control": "textbox",
      
        "disabled": true
      },
      {
        "id": "origin",
        "label": "Origin Location",
        "control": "select",
        "populateURL": "/Service/origin.json",
        "required": "true"
      },
      {
        "id": "destination",
        "label": "Destination Location",
        "control": "select",
        "populateURL": "/Service/destination.json",
        "required": "true"
      },
      {
        "id": "date",
        "label": "Expected Date",
        "control": "date",
        "validate": "datetime",
        "required": "true"
      },
      {
        "id": "address",
        "label": "Address",
        "control": "textbox",
        "validate": "alphanumeric",
        "required": "true"
      },
      {
        "id": "email",
        "label": "Email",
        "control": "textbox",
        "validate": "email",
        "required": "true"
      },
      {
        "id": "production",
        "label": "Production",
        "control": "textbox",
        "validate": "aadhar",
        "required": "true"
      },
      {
        "id": "membership",
        "label": "Membership",
        "control": "textbox",
        "validate": "pan",
        "required": "true"
      },
      {
        "id": "warehouse",
        "label": "WareHouse Service",
        "control": "textbox",
        "validate": "alphanumeric",
        "required": "true"
      },
      {
        "id": "truckload",
        "label": "TruckLoad Service",
        "control": "textbox",
        "validate": "alphanumeric",
        "required": "true"
      },
      {
        "id": "price",
        "label": "Price",
        "control": "textbox",
        "validate": "price",
        "required": "true"
      },
      {
        "id": "contact",
        "label": "Contact",
        "control": "textbox",
        "validate": "phone",
        "required": "true"
      }
    ],
    "buttons": [
      {
        "id": "create",
        "label": "Create PO",
        "control": "button",
        "validationjs": "validatedata",
        "submitURL": "https://localhost/createPO"
      },
      {
        "id": "cancel",
        "label": "Cancel",
        "control": "button",
        "redirect": "polist"
      }
    ]
  }
}